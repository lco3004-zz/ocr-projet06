<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="fr.ocr.prj06.tstdao.dbUtilisateurEntity">
        <table name="utilisateur" schema="ocr_projet06" catalog=""/>
        <attributes>
            <id name="idUtilsateur">
                <column name="id_utilsateur" precision="11"/>
            </id>
            <basic name="nom">
                <column name="nom" length="45"/>
            </basic>
            <basic name="email">
                <column name="email" length="45"/>
            </basic>
            <many-to-one name="compteUtilisateurByFkCompteUtilisateurId" optional="true"
                         target-entity="fr.ocr.prj06.tstdao.dbCompteUtilisateurEntity">
                <join-column name="fk_compte_utilisateur_id" referenced-column-name="id_compte"/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
