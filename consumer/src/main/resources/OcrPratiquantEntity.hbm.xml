<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.ocr.prj06.hibernate.stubs.OcrPratiquantEntity" table="ocr_pratiquant" schema="ocr_projet06">
        <id name="idocrPratiquant">
            <column name="idocr_pratiquant" sql-type="int(11)"/>
        </id>
        <property name="ocrPratiquantNom">
            <column name="ocr_pratiquant_nom" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="ocrPratiquantEmail">
            <column name="ocr_pratiquant_email" sql-type="varchar(45)" length="45"/>
        </property>
        <many-to-one name="ocrCompteUtilisateurByFkOcrCompteUtilisateurId"
                     class="fr.ocr.prj06.hibernate.stubs.OcrCompteUtilisateurEntity">
            <column name="fk_ocr_compte_utilisateur_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>